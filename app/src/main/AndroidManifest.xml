<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test_aja">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/bsi_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bsi_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Testaja">
        <activity android:name=".services"></activity>
        <activity
            android:name=".HomeActivity"
            android:label="Home"></activity>
        <activity android:name=".tws_view" />
        <activity android:name=".switch_view" />
        <activity android:name=".db_view" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Testaja.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>